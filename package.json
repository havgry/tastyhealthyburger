{
  "name": "tastyhealthyburger",
  "private": false,
  "scripts": {
    "buildDev": "webpack-dev-server --inline --hot --port 3333",
    "build": "cross-env NODE_ENV=production webpack --hide-modules --optimize-minimize",
    "clean": "rm -rf dist assets/* && mkdir dist",
    "release": "npm run clean && npm run build && cp -R assets dist/assets && cp *.html CNAME dist/",
    "deploy": "npm run release && gh-pages -d dist -m 'Create release'"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.4.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-standard": "^3.0.1",
    "file-loader": "^0.11.2",
    "gh-pages": "^1.0.0",
    "node-sass": "^4.5.3",
    "postcss-cssnext": "^3.0.2",
    "sass-loader": "^6.0.6",
    "vue-loader": "^13.0.4",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.6.1",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link": "^1.0.7",
    "apollo-link-batch-http": "^1.0.4",
    "graphql": "^0.12.3",
    "graphql-tag": "^2.6.1",
    "lodash": "^4.17.4",
    "normalize.css": "^7.0.0",
    "vue": "^2.4.2",
    "vue-apollo": "^3.0.0-alpha.2",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  }
}
